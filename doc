Retrieve SID:

SERVICE-ID
SERVICE-ID-RQ
SERVICE-ID-RSP
SERVICE-ID-RES
POS-SERVICE-ID
NEG-SERVICE-ID

function findSIDParam(params = []) {
  return params.find(p => {
    const s = p.semantic?.toUpperCase() || "";
    const n = p.shortName?.toUpperCase() || "";

    return (
      s === "SERVICE-ID" ||
      s.includes("SERVICE-ID") ||
      s === "SID" ||
      n.includes("SID")
    );
  });
}
